CREATE OR REPLACE FUNCTION migracion.f_trans_tkp_empleado_afp_tfuncionario_afp (
  v_operacion varchar,
  p_id_empleado_afp integer,
  p_id_afp integer,
  p_id_empleado integer,
  p_estado_reg varchar,
  p_fecha_asignacion date,
  p_fecha_finalizacion date,
  p_fecha_reg date,
  p_jubilado varchar,
  p_nro_afp varchar
)
RETURNS varchar [] AS
$body$
DECLARE
			 
			g_registros record;
			v_consulta varchar;
			v_res_cone  varchar;
			v_cadena_cnx varchar;
			v_cadena_con varchar;
			resp boolean;
			v_resp varchar;
			v_respuesta varchar[];
			
			g_registros_resp record;
			v_id_funcionario_afp int4;
			v_id_afp int4;
			v_estado_reg varchar;
			v_fecha_fin date;
			v_fecha_ini date;
			v_fecha_mod timestamp;
			v_fecha_reg timestamp;
			v_id_funcionario int4;
			v_id_usuario_ai int4;
			v_id_usuario_mod int4;
			v_id_usuario_reg int4;
			v_nro_afp varchar;
			v_tipo_jubilado varchar;
			v_usuario_ai varchar;
BEGIN
			
			
			          --funcion para obtener cadena de conexion
			          v_cadena_cnx =  migracion.f_obtener_cadena_con_dblink();
			          
			          
			           ---------------------------------------
			           --previamente se tranforman los datos  (descomentar)
			           ---------------------------------------

			           
			           			v_id_funcionario_afp=p_id_empleado_afp::int4;
			v_id_afp=p_id_afp::int4;
			v_estado_reg=convert(p_estado_reg::varchar, 'LATIN1', 'UTF8');
			v_fecha_fin=p_fecha_finalizacion::date;
			v_fecha_ini=p_fecha_asignacion::date;
			v_fecha_mod=now()::timestamp;
			v_fecha_reg=p_fecha_reg::timestamp;
			v_id_funcionario=p_id_empleado::int4;
			v_id_usuario_ai=1::int4;
			v_id_usuario_mod=1::int4;
			v_id_usuario_reg=1::int4;
			v_nro_afp=convert(p_nro_afp::varchar, 'LATIN1', 'UTF8');
			v_tipo_jubilado=convert(p_jubilado::varchar, 'LATIN1', 'UTF8');
			v_usuario_ai=convert('no_hay'::varchar, 'LATIN1', 'UTF8');
   
			    --cadena para la llamada a la funcion de insercion en la base de datos destino
			      
			        
			          v_consulta = 'select migra.f__on_trig_tkp_empleado_afp_tfuncionario_afp (
			               '''||v_operacion::varchar||''','||COALESCE(v_id_funcionario_afp::varchar,'NULL')||','||COALESCE(v_id_afp::varchar,'NULL')||','||COALESCE(''''||v_estado_reg::varchar||'''','NULL')||','||COALESCE(''''||v_fecha_fin::varchar||'''','NULL')||','||COALESCE(''''||v_fecha_ini::varchar||'''','NULL')||','||COALESCE(''''||v_fecha_mod::varchar||'''','NULL')||','||COALESCE(''''||v_fecha_reg::varchar||'''','NULL')||','||COALESCE(v_id_funcionario::varchar,'NULL')||','||COALESCE(v_id_usuario_ai::varchar,'NULL')||','||COALESCE(v_id_usuario_mod::varchar,'NULL')||','||COALESCE(v_id_usuario_reg::varchar,'NULL')||','||COALESCE(''''||v_nro_afp::varchar||'''','NULL')||','||COALESCE(''''||v_tipo_jubilado::varchar||'''','NULL')||','||COALESCE(''''||v_usuario_ai::varchar||'''','NULL')||')';
			          --probar la conexion con dblink
			          
					   --probar la conexion con dblink
			          v_resp =  (SELECT dblink_connect(v_cadena_cnx));
			            
			             IF(v_resp!='OK') THEN
			            
			             	--modificar bandera de fallo  
			                 raise exception 'FALLA CONEXION A LA BASE DE DATOS CON DBLINK';
			                 
			             ELSE
					  
			         
			               PERFORM * FROM dblink(v_consulta,true) AS ( xx varchar);
			                v_res_cone=(select dblink_disconnect());
			             END IF;
			            
			            v_respuesta[1]='TRUE';
                       
			           RETURN v_respuesta;
			EXCEPTION
			   WHEN others THEN
			   
			    v_res_cone=(select dblink_disconnect());
			     v_respuesta[1]='FALSE';
                 v_respuesta[2]=SQLERRM;
                 v_respuesta[3]=SQLSTATE;
                 v_respuesta[4]=v_consulta;
                 
    
                 
                 RETURN v_respuesta;
			
			END;
$body$
LANGUAGE 'plpgsql'
VOLATILE
CALLED ON NULL INPUT
SECURITY INVOKER;